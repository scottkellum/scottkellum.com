<%= partial "partials/head" %>
    
    <article id="main" role="main">
      <h1><%= data.page.title %></h1>
      <%= yield %>
    </article>
    
    <aside>
      <h2>By Year</h2>
      <ol class="articles-cal">
        <% blog.articles.group_by {|a| a.date.year }.each do |year, articles| %>
          <li><%= link_to year, blog_year_path(year) %> <span class="count"><%= articles.size %></span></a></li>
        <% end %>
      </ol>
    </aside>
    <%= partial "partials/footer" %>
  </body>

  <!-- WEBTYPE -->
  <link href="//cloud.webtype.com/css/5fd75aea-fa83-43ad-8fb7-509ad91b9cd7.css" rel="stylesheet" type="text/css" />

  <!-- TYPEKIT -->
  <script type="text/javascript" src="//use.typekit.net/qfs2thr.js"></script>
  <script type="text/javascript">try{Typekit.load();}catch(e){}</script>

</html>
